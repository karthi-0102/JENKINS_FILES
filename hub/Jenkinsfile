pipeline{
    agent any
    stages{
        stage('unit test code'){
            steps{
                sh 'mkdir -p /tmp/hub-buildDir/hub'
                dir('/tmp/hub-buildDir/hub'){
                    checkout scm
                    sh 'ls'
                    sh 'cd hub && ls'
                    sh 'find ./ -name Jenkinsfile'
                    sh 'echo hii'
                     
                }
            }   
                
        }
    }
}
